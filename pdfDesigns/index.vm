<body>
<header>
    <div class="container">
        <img class="corp-logo pull-left" src="template/assets/img/corp-logo.png"></img>

        <div class="pull-right text-right">
            <div class="user-name"> $name </div>
            <div class="user-email"> $email </div>
        </div>
        <div class="clearfix"></div>
    </div>
</header>
<div class="header-down">
    <div class="container">
        <table class="header-table">
            <tr></tr>
            <tr></tr>
            <tr></tr>
            <tr></tr>
            <tr></tr>
            <tr>
                <td width="50%">
                    <div class="pull-left">
                        <div class="top-cat">Trip Report For</div>
                        <div class="cat-val date">JAN 16</div>
                    </div>
                </td>
                <td width="">
                    <div class="rest-cat pull-right text-right">
                        <div class="top-cat">Balance</div>
                        <div class="cat-val">&#8377;3153</div>
                    </div>
                </td>
                <td width="">
                    <div class="rest-cat pull-right text-right">
                        <div class="top-cat">Expense</div>
                        <div class="cat-val">&#8377;4557</div>
                    </div>
                </td>
                <td width="">
                    <div class="rest-cat pull-right text-right">
                        <div class="top-cat">Rides</div>
                        <div class="cat-val">$numberOfRides</div>
                    </div>
                </td>
            </tr>
            <tr></tr>
            <tr></tr>
            <tr></tr>
            <tr></tr>
            <tr></tr>
        </table>
    </div>
</div>

<section>
    <div class="container">
        #foreach( $ride in $rides )
            <div class="trip-blk">
                <div class="col-lg-10 col-md-9 col-sm-9 col-xs-12">
                    <img class="car-icon sedan" src="template/assets/img/sedan.png"></img>

                    <div class="trip-det">
                        <div class="det-blk">
                            <div class="trip-date">2nd January, Monday 11:00 AM</div>
                            <div class="car-type">$data.get("cab_category")</div>
                            <div class="car-sep"></div>
                            <div class="crn-no">CRN $data.get("crn_number")</div>
                        </div>

                        <div class="det-blk">
                            <div class="reason-title">Ride Reason</div>
                            <div class="ride-reason">
                                #if ($data.get("reason"))
                                    $data.get("reason")
                                #else
                                    N/A
                                #end
                            </div>
                        </div>
                        <div class="det-blk ride-summary">
                            <div class="transit-line"></div>
                            <ul class="ride-address">
                                <li>
                                    #if ($data.get("pickup_area"))
                                        $data.get("pickup_area")
                                    #else
                                        N/A
                                    #end
                                </li>
                                <li>
                                    #if ($data.get("drop_location"))
                                        $data.get("drop_location")
                                    #else
                                        N/A
                                    #end
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col-lg-2 col-md-2 col-sm-3 col-xs-12 price-blk pull-right">
                    <div class="trip-fare">&#8377;
                        #if ($data.get("bill"))
                            $data.get("bill")
                        #else
                            N/A
                        #end
                    </div>
                    <div class="trip-type prepaid">Prepaid</div>
                </div>
                <div class="clearfix"></div>
            </div>
        #end
    </div>
</section>

<footer>
    <div class="container">
        <div class="pull-left">OLA CORPORATE MONTHLY REPORT</div>
        <div class="pull-right">Page 1 of 5</div>
    </div>
</footer>
</body>
